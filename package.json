{
  "name": "@abstractplay/xbetza",
  "version": "1.0.0-beta",
  "description": "An XBetza notation parser",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc && npm pack",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "lint": "npx eslint .",
    "dist": "rimraf dist && webpack --mode=production",
    "full": "npm run test && npm run build && npm run dist",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "AbstractPlay/xbetza"
  },
  "author": "Aaron Dalton <aaron@daltons.ca> (https://www.perlkonig.com/)",
  "license": "MIT",
  "keywords": [
    "abstractplay",
    "games",
    "xbetza",
    "chess"
  ],
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.10",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "chai": "^4.3.7",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^59.0.1",
    "json-schema-to-typescript": "^15.0.4",
    "mocha": "^11.2.2",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.1",
    "shx": "^0.4.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "rfdc": "^1.3.0"
  }
}
